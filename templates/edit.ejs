<!-- favoritos -->
<div class="row">
	<div class="col s12">
		<div class="input-field col s12">
			<select id="favoritos" multiple>
				<option value="" disabled>Servicios favoritos</option>
				<% services.forEach(function(item) { %>
					<% var selected = favorites && favorites.indexOf(item.name) !== -1 ? 'selected' : '' %>
					<option <%= selected %> value="<%= item.name %>" ><%= item.caption %></option>
				<% }) %>
			</select>
			<label>Escoja sus cuatro servicios favoritos</label>
		</div>
	</div>
</div>

